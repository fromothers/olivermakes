---
---
{
  "type": "FeatureCollection",
  "features": [
    {% for photo in site.photos %}
    {
      "type": "Feature",
      "properties": {
        "title": "{{ photo.title }}",
        "url": "{{ photo.url }}",
        "src": "{{ photo.src | imgix_url: w: 144, h: 96, q: 45, fit: crop }}",
        "alt": "{{ photo.alt }}",
        "iconSize": [72, 48]
      },
      "geometry": {
        "type": "Point",
        "coordinates": [{{ photo.longitude }}, {{ photo.latitude }}]
      }
    }{% if forloop.rindex > 1 %},{% endif%}
    {% endfor %}
  ]
}
